{
  "name": "devrsi0n-blog",
  "private": true,
  "description": "Devrsi0n's personal blog",
  "version": "1.0.0",
  "author": "devrsi0n <devrsi0n@gmail.com>",
  "homepage": "https://devrsi0n.com",
  "keywords": [
    "devrsi0n",
    "gatsby",
    "zeit",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "types": "./src/types/index.d.ts",
  "scripts": {
    "prestart": "gatsby clean",
    "dev": "gatsby develop -o",
    "start": "concurrently \"yarn run dev\" \"delay 60 && yarn run codegen\"",
    "clean": "gatsby clean",
    "build": "yarn run clean && gatsby build",
    "serve": "gatsby serve",
    "analyze": "cross-env ANALYZE=true gatsby build",
    "lint": "yarn run eslint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "prettier": "prettier --write ./**/*.{json,html,css,scss,md,yml}",
    "now-dev": "gatsby develop -p $PORT",
    "now-build": "node ./now-build.js",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "contentful:export": "contentful space export --config contentful.json --skip-content --skip-roles --skip-webhooks",
    "contentful:export:model": "contentful space export --config contentful.json",
    "contentful:import": "contentful space import --config contentful.json"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "@theme-ui/color": "^0.2.53",
    "@theme-ui/components": "^0.2.50",
    "@theme-ui/preset-deep": "^0.2.40",
    "@theme-ui/prism": "^0.2.50",
    "contentful-cli": "^0.36.2",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "gatsby": "^2.19.22",
    "gatsby-image": "^2.2.41",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-catch-links": "^2.1.26",
    "gatsby-plugin-emotion": "^4.1.22",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-feed": "^2.3.27",
    "gatsby-plugin-google-analytics": "^2.1.36",
    "gatsby-plugin-layout": "^1.1.22",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-mdx": "^1.0.74",
    "gatsby-plugin-nprogress": "^2.1.19",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-theme-ui": "^0.2.53",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-plugin-typography": "^2.3.22",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-autolink-headers": "^2.1.24",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.37",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-numbered-footnotes": "^1.0.1",
    "gatsby-remark-prismjs": "^3.3.31",
    "gatsby-remark-responsive-iframe": "^2.2.32",
    "gatsby-remark-smartypants": "^2.1.21",
    "gatsby-remark-typescript": "^0.2.2",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.53",
    "gatsby-transformer-sharp": "^2.3.16",
    "gatsby-transformer-yaml": "^2.2.25",
    "gh-pages": "^2.2.0",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.28",
    "node-fetch": "^2.6.0",
    "polished": "^3.4.4",
    "prism-react-renderer": "^1.0.2",
    "prismjs": "^1.19.0",
    "query-string": "^6.11.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.2",
    "react-medium-image-zoom": "^3.1.2",
    "react-outside-click-handler": "1.0.0",
    "react-transition-group": "^4.3.0",
    "react-typography": "^0.16.19",
    "remark-slug": "^5.1.2",
    "semver": "^7.1.3",
    "theme-ui": "^0.2.52",
    "typeface-merriweather": "0.0.72"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/introspection": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "@types/lodash": "^4.14.147",
    "@types/node": "^12.12.28",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/theme-ui": "^0.2.8",
    "concurrently": "^5.1.0",
    "delay-cli": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettify-ts-react": "^0.1.8",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "~1.19.1",
    "typescript": "~3.8.2"
  },
  "browserslist": [
    "chrome >= 57",
    "firefox >= 52",
    "edge >= 16",
    "safari >= 10.1",
    "opera >= 44",
    "ios >= 10.3",
    "ChromeAndroid >= 57"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,scss,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": "12.x"
  }
}
