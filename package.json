{
  "name": "devrsi0n-blog",
  "private": true,
  "description": "Devrsi0n's personal blog",
  "version": "1.0.0",
  "author": "devrsi0n <devrsi0n@gmail.com>",
  "homepage": "https://devrsi0n.com",
  "keywords": [
    "devrsi0n",
    "gatsby",
    "netlify",
    "zeit",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "types": "./src/types/index.d.ts",
  "scripts": {
    "prestart": "gatsby clean",
    "start": "gatsby develop -o",
    "clean": "gatsby clean",
    "build": "npm run clean && gatsby build",
    "serve": "gatsby serve",
    "analyze": "cross-env ANALYZE=true gatsby build",
    "lint": "npm run eslint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "prettier": "prettier --write ./**/*.{json,html,css,scss,md,yml}",
    "deploy": "gh-pages -r git@github.com:devrsi0n/devrsi0n.github.io.git -b master -d public",
    "now-dev": "gatsby develop -p $PORT",
    "now-build": "npm run build",
    "contentful:export": "contentful space export --config contentful.json --skip-content --skip-roles --skip-webhooks",
    "contentful:export:model": "contentful space export --config contentful.json",
    "contentful:import": "contentful space import --config contentful.json"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.22",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "adm-zip": "^0.4.13",
    "babel-plugin-emotion": "^10.0.22",
    "contentful-cli": "^0.36.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "gatsby": "^2.17.2",
    "gatsby-image": "^2.2.29",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-catch-links": "^2.1.15",
    "gatsby-plugin-emotion": "^4.1.12",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-feed": "^2.3.19",
    "gatsby-plugin-google-analytics": "^2.1.23",
    "gatsby-plugin-layout": "^1.1.13",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-mdx": "^1.0.53",
    "gatsby-plugin-nprogress": "^2.1.12",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-plugin-typescript": "^2.1.15",
    "gatsby-plugin-typography": "^2.3.14",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-autolink-headers": "^2.1.16",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.28",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.1.28",
    "gatsby-remark-numbered-footnotes": "^1.0.1",
    "gatsby-remark-prismjs": "^3.3.20",
    "gatsby-remark-responsive-iframe": "^2.2.25",
    "gatsby-remark-smartypants": "^2.1.14",
    "gatsby-remark-typescript": "^0.0.15",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-remark": "^2.6.30",
    "gatsby-transformer-sharp": "^2.2.19",
    "gatsby-transformer-yaml": "^2.2.15",
    "gitalk": "^1.5.0",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.27",
    "prism-react-renderer": "^0.1.7",
    "prismjs": "^1.17.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.1",
    "react-medium-image-zoom": "^3.1.2",
    "react-outside-click-handler": "^1.3.0",
    "react-transition-group": "^4.3.0",
    "react-typography": "^0.16.19",
    "remark-slug": "^5.1.2",
    "request": "^2.88.0",
    "theme-ui": "^0.2.44",
    "typeface-merriweather": "0.0.72",
    "typography": "^0.16.19",
    "typography-theme-pottery": "^0.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.11.6",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-helmet": "^5.0.13",
    "@types/theme-ui": "^0.2.3",
    "eslint": "^6.6.0",
    "eslint-config-prettify-ts-react": "^0.1.3",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "~1.18.2",
    "typescript": "~3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,scss,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": "10.x || 12.x"
  }
}
