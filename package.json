    "theme-ui": "^0.2.46",
    "typeface-merriweather": "0.0.72",
    "typography": "^0.16.19",
    "typography-theme-pottery": "^0.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/lodash": "^4.14.147",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/theme-ui": "^0.2.4",
    "eslint": "^6.6.0",
    "eslint-config-prettify-ts-react": "^0.1.6",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "polished": "^3.4.2",
    "prettier": "~1.19.1",
    "typescript": "~3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,scss,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": "10.x || 12.x"
  }
}
