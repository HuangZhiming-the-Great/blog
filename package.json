  "dependencies": {
    "babel-plugin-styled-components": "^1.9.2",
    "gatsby": "^2.0.64",
    "gatsby-image": "^2.0.22",
    "gatsby-link": "^2.0.7",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-netlify-cms": "^3.0.9",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sharp": "^2.0.15",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-typography": "^2.2.2",
    "gatsby-remark-copy-linked-files": "^2.0.7",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.11",
    "gatsby-transformer-remark": "^2.1.15",
    "gatsby-transformer-sharp": "^2.1.9",
    "gray-percentage": "^2.0.0",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.5",
    "netlify-cms": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.18",
    "styled-components": "^4.1.2",
    "typography": "^0.16.18",
    "typography-breakpoint-constants": "^0.16.18",
    "typography-theme-wordpress-2016": "^0.16.18",
    "uuid": "^3.3.2"
  },
  "keywords": [
    "gatsby",
    "netlify",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "yarn run develop",
    "clean": "rimraf .cache public",
    "build": "yarn run clean && gatsby build",
    "develop": "yarn run clean && gatsby develop",
    "serve": "gatsby serve",
    "lint": "yarn run eslint && yarn run prettier",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint --fix src/",
    "prettier": "prettier --write ./**/*.{json,html,css,scss,md,yml}"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@devrsi0n/eslint-config-react": "^0.1.1",
    "eslint": "^5.10.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
