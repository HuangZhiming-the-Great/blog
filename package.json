{
  "name": "devrsi0n-blog",
  "private": true,
  "description": "Devrsi0n's personal blog",
  "version": "1.0.0",
  "author": "devrsi0n <devrsi0n@gmail.com>",
  "homepage": "https://devrsi0n.com",
  "keywords": [
    "devrsi0n",
    "gatsby",
    "netlify",
    "zeit",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "types": "./src/types/index.d.ts",
  "scripts": {
    "prestart": "gatsby clean",
    "start": "gatsby develop -o",
    "clean": "gatsby clean",
    "build": "yarn run clean && gatsby build",
    "serve": "gatsby serve",
    "analyze": "cross-env ANALYZE=true gatsby build",
    "lint": "yarn run eslint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "prettier": "prettier --write ./**/*.{json,html,css,scss,md,yml}",
    "now-dev": "gatsby develop -p $PORT",
    "now-build": "node ./now-build.js",
    "contentful:export": "contentful space export --config contentful.json --skip-content --skip-roles --skip-webhooks",
    "contentful:export:model": "contentful space export --config contentful.json",
    "contentful:import": "contentful space import --config contentful.json"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "adm-zip": "^0.4.13",
    "babel-plugin-emotion": "^10.0.23",
    "contentful-cli": "^0.36.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "gatsby": "^2.17.13",
    "gatsby-image": "^2.2.32",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-catch-links": "^2.1.16",
    "gatsby-plugin-emotion": "^4.1.14",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-feed": "^2.3.20",
    "gatsby-plugin-google-analytics": "^2.1.26",
    "gatsby-plugin-layout": "^1.1.14",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^2.2.27",
    "gatsby-plugin-mdx": "^1.0.56",
    "gatsby-plugin-nprogress": "^2.1.13",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-sharp": "^2.2.38",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-plugin-typescript": "^2.1.17",
    "gatsby-plugin-typography": "^2.3.16",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-autolink-headers": "^2.1.17",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.29",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.1.30",
    "gatsby-remark-numbered-footnotes": "^1.0.1",
