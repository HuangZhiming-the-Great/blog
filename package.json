  "homepage": "https://devrsi0n.me",
  "dependencies": {
    "cross-env": "^5.2.0",
    "gatsby": "^2.0.91",
    "gatsby-plugin-feed": "^2.0.11",
    "gatsby-plugin-google-analytics": "^2.0.9",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-plugin-typography": "^2.2.5",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.4",
    "gatsby-remark-autolink-headers": "^2.0.12",
    "gatsby-remark-copy-linked-files": "^2.0.8",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.2.0",
    "gatsby-remark-responsive-iframe": "^2.0.8",
    "gatsby-remark-smartypants": "^2.0.7",
    "gatsby-source-filesystem": "^2.0.16",
    "gatsby-transformer-remark": "^2.2.0",
    "gatsby-transformer-sharp": "^2.1.10",
    "gitalk": "^1.4.1",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "node-sass": "^4.11.0",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.18",
    "typography": "^0.16.18",
    "typography-theme-pottery": "^0.0.2"
  },
  "keywords": [
    "gatsby",
    "netlify",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf .cache public",
    "build": "npm run clean && gatsby build",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "analyze": "cross-env ANALYZE=true gatsby build",
    "lint": "npm run eslint && npm run prettier",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint --fix src/",
    "prettier": "prettier --write ./**/*.{json,html,css,scss,md,yml}",
    "now-build": "npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.3.1",
    "@commitlint/config-conventional": "^7.3.1",
    "@devrsi0n/eslint-config-react": "^0.1.1",
    "eslint": "^5.12.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
