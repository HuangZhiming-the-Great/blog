    "gatsby-remark-smartypants": "^2.1.15",
    "gatsby-remark-typescript": "^0.0.15",
    "gatsby-source-filesystem": "^2.1.36",
    "gatsby-transformer-remark": "^2.6.34",
    "gatsby-transformer-sharp": "^2.2.19",
    "gatsby-transformer-yaml": "^2.2.16",
    "gh-pages": "^2.1.1",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.27",
    "node-fetch": "^2.6.0",
    "prism-react-renderer": "^1.0.2",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
